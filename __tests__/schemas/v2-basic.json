{
  "swagger": "2.0",
  "info": {
    "title": "Simple Swagger 2 API",
    "version": "1.0.0"
  },
  "securityDefinitions": {
    "securityUser": {
      "type": "oauth2",
      "authorizationUrl": "https://localhost/auth/authorize",
      "flow": "implicit",
      "scopes": {
        "scope1": "Scope 1 description",
        "scope2": "Scope 2 description"
      }
    }
  },
  "paths": {
    "/path/{path_parameter_1}/segment/{path_parameter_2}/{path_parameter_3}": {
      "parameters": [
        {
          "in": "path",
          "name": "path_parameter_1",
          "required": true,
          "type": "string"
        },
        {
          "in": "path",
          "name": "path_parameter_2",
          "required": true,
          "type": "string"
        },
        {
          "in": "path",
          "name": "path_parameter_3",
          "required": true,
          "type": "string"
        }
      ],
      "get": {
        "produces": ["application/json"],
        "parameters": [
          {
            "required": true,
            "name": "queryParam1",
            "in": "query",
            "type": "string"
          },
          {
            "name": "queryParam2",
            "in": "query",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          {
            "name": "queryParam3",
            "in": "query",
            "type": "string",
            "enum": ["ACTIVE", "INACTIVE"],
            "default": "ACTIVE"
          }
        ],
        "responses": {
          "200": {
            "description": "Just simple get operation",
            "schema": {
              "properties": {
                "items": {
                  "items": {
                    "additionalProperties": false,
                    "properties": {
                      "_id": {
                        "readOnly": true,
                        "type": "string"
                      },
                      "date": {
                        "format": "date-time",
                        "type": "string"
                      },
                      "name": {
                        "readOnly": true,
                        "type": "string"
                      },
                      "ttl": {
                        "type": "integer"
                      }
                    },
                    "type": "object"
                  },
                  "type": "array"
                }
              },
              "type": "object"
            }
          },
          "default": {
            "description": "Generic error response",
            "schema": {
              "properties": {
                "description": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "payload": {
                  "type": "object"
                },
                "status": {
                  "type": "integer"
                },
                "userinfo": {
                  "type": "string"
                }
              },
              "required": ["status", "name"],
              "type": "object"
            }
          }
        },
        "operationId": "simpleGetOperation"
      },
      "post": {
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "security": [
          {
            "securityUser": ["scope1"]
          }
        ],
        "parameters": [
          {
            "in": "body",
            "name": "data",
            "required": true,
            "schema": {
              "additionalProperties": false,
              "properties": {
                "_id": {
                  "readOnly": true,
                  "type": "string"
                },
                "date": {
                  "format": "date-time",
                  "type": "string"
                },
                "name": {
                  "readOnly": true,
                  "type": "string"
                },
                "ttl": {
                  "type": "integer"
                }
              },
              "type": "object"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Something created",
            "schema": {
              "additionalProperties": false,
              "properties": {
                "_id": {
                  "readOnly": true,
                  "type": "string"
                }
              },
              "type": "object"
            }
          },
          "409": {
            "description": "Something already exists",
            "schema": {
              "properties": {
                "description": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "payload": {
                  "type": "object"
                },
                "status": {
                  "type": "integer"
                },
                "userinfo": {
                  "type": "string"
                }
              },
              "required": ["status", "name"],
              "type": "object"
            }
          },
          "default": {
            "description": "Generic error response",
            "schema": {
              "properties": {
                "description": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "payload": {
                  "type": "object"
                },
                "status": {
                  "type": "integer"
                },
                "userinfo": {
                  "type": "string"
                }
              },
              "required": ["status", "name"],
              "type": "object"
            }
          }
        },
        "operationId": "simplePostOperation"
      },
      "x-exegesis-controller": "controllers/ctrl"
    }
  }
}
